{
  "manifest_version": 3,
  "name": "Reminder Redirector",
  "description": "Base Level Extension",
  "version": "1.1",
  "icons": {
    "16": "redirect.png",
    "48": "redirect.png",
    "128": "redirect.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "redirect.png"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": ["alarms", "storage"]
}